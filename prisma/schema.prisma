generator client {
  provider = "prisma-client-js"
}

generator prismabox {
  provider                    = "prismabox"
  output                      = "../src/core/data-access/prisma"
  typeboxImportVariableName   = "t"
  typeboxImportDependencyName = "elysia"
  inputModel                  = true
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(cuid())
  username  String  @unique
  name      String?
  password  String?
  avatarUrl String?
}
